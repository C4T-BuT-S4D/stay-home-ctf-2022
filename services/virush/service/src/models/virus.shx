#!/usr/bin/shx

@module models/virus || {
    @require logging

    @class virus

    function virus::init {
        log::enter_function

        __name=$1
        __dna=$2
        __description=$3

        log::exit_function
    }

    function virus::test {
        log::enter_function

        log::info $__data1
        log::info $__data2

        log::exit_function
    }
}
